{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
      "interface-name": [false],
      "no-console": false,
      "curly": [true, "ignore-same-line"],
      "object-literal-sort-keys": false,
      "no-string-throw": false,
      "quotemark": false,
      "trailing-comma": false,
      "max-line-length": [true, 120]
    },
    "rulesDirectory": []
  }
  